{
  "name": "whozwho-client",
  "version": "1.2.1",
  "description": "TypeScript client library for WhozWho service",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "bump": "VERSION=\"$(npm version patch --no-git-tag-version)\"",
    "update-changelog": "node ./scripts/update-changelog-date.js",
    "build": "tsc && cp README.md dist/ && cp package.json dist/",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "release": "git add . && git commit -m \"chore: release v$(node -p \"require('./package.json').version\")\" && git push origin main",
    "release-with-bump": "npm run bump && npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlefree/whozwho-client.git"
  },
  "keywords": [
    "whozwho",
    "client",
    "typescript"
  ],
  "author": "mlefree",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlefree/whozwho-client/issues"
  },
  "homepage": "https://github.com/mlefree/whozwho-client#readme",
  "dependencies": {
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  }
}
